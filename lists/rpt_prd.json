            "primaryKey": "id",
            "sql": "SELECT det.c_nome_banco AS Banco,\nato.c_status AS Status, Month(ato.c_dataOutorga) as Mes, ato.c_motivo_desmarcacao_ato, COUNT(det.c_id_processo) AS Processos\nFROM app_fd_processos pro\nLEFT JOIN app_fd_detalhe_processo det ON pro.c_detalhe_processo = det.id\nLEFT JOIN app_fd_agendamento ag ON pro.c_agendamento = ag.id\nLEFT JOIN app_fd_atos ato ON ato.c_id_agendamento = ag.id\nWHERE ato.c_status != ''\nGROUP BY det.c_nome_banco,ato.c_status, Mes, ato.c_motivo_desmarcacao_ato\n\n\n\n"
        }
