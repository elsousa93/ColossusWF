            "optimisePaging": "",
            "primaryKey": "procId",
            "sql": "SELECT p.id as procId, dp.c_n_wf_banco as N_WF_Banco,dp.c_categoria_processo as CategoriaProcesso, dp.c_processo_vip AS VIP, dp.c_tipo_relacao AS tipoRelacao, dp.c_n_wf_pl as N_WF_PL, dp.c_nome_banco AS NomeBanco, dp.c_n_wf_pl_relacionado AS ProcessosRelacionados, dp.c_urgencia_registo AS Urgencia, ip.c_nome as NomeCliente, dp.c_tipo_processo as TipoProcesso, sp.Id AS processId, sp.Name AS processName, sact.Id AS instanceId, sact.Name AS activityName, sass.ResourceId AS assignee, p.dateCreated, dp.c_tipo_entidade as Entidade,  atos.c_status as EstadoAgendamento, atos.c_cedula_entidade as entidadeAutenticadora, CONCAT(atos.c_dataOutorga, ' ', atos.c_horaOutorga) AS DataAgendamento, ROW_NUMBER() OVER (ORDER BY atos.c_dataOutorga ASC, atos.c_horaOutorga ASC, dp.c_urgencia_registo DESC) AS No\nFROM shkprocesses sp\nLEFT JOIN shkprocesses sp2 on sp.ActivityRequesterProcessId = sp2.Id\nLEFT JOIN app_fd_processos p on sp.ActivityRequesterProcessId = p.id\nLEFT JOIN app_fd_detalhe_processo dp ON dp.id = p.c_detalhe_processo\nLEFT JOIN shkprocessstates sps ON sps.oid = sp.State\nLEFT JOIN SHKActivities sact ON sact.ProcessId = sp.Id\nLEFT JOIN SHKActivityStates ssta ON ssta.oid = sact.State\nLEFT JOIN (select ActivityId, group_concat(ResourceId separator '; ') as ResourceId from SHKAssignmentsTable group by ActivityId) sass on sact.Id = sass.ActivityId\nLEFT JOIN app_fd_intervenientes_proc ip ON ip.c_id_processo = p.id AND ip.c_cliente_principal = 'CHECK'\nLEFT JOIN app_fd_agendamento ag ON  ag.id = p.c_agendamento\nLEFT JOIN app_fd_atos atos ON atos.c_id_agendamento = ag.id AND atos.c_status = 'OUT'\nWHERE sps.KeyValue LIKE 'open.running'\nAND p.id is not null\nAND (ssta.oid = '1000001' OR ssta.oid = '1000003' OR ssta.oid = '1000005')\nAND (sact.Id like '%deposito'AND sact.Id not like '%pagamentoDeposito') \nAND sass.ResourceId LIKE \"%registos_geral%\"\nORDER BY DataAgendamento ASC, Urgencia DESC"
        }
    },
