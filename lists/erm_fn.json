            "primaryKey": "id",
            "sql": "SELECT pro.c_servico AS Servico, det.c_id_processo AS IDProc, det.c_nome_banco AS Banco, \ndet.c_forma_outorga as Forma_outorga, det.c_n_wf_pl as WF_PL,\nabanc.c_data_emissao_fine AS EmissaoFINE, abanc.c_origem_fine AS OrigemFINE, abanc.c_estado_processo_fine AS Estado\nFROM app_fd_processos pro\nLEFT JOIN app_fd_detalhe_processo det ON pro.c_detalhe_processo = det.id\nLEFT JOIN app_fd_anal_banc_processo abanc ON det.c_id_processo = abanc.c_id_processo\nWHERE abanc.c_data_emissao_fine > 0\nUNION\nSELECT pro.c_servico AS Servico, det.c_id_processo AS IDProc, det.c_nome_banco AS Banco, \ndet.c_forma_outorga as Forma_outorga, det.c_n_wf_pl as WF_PL,\nabanc.c_data_2_emissao_fine AS EmissaoFINE, abanc.c_origem_fine_2 AS OrigemFINE, 'Not Ok' AS Estado\nFROM app_fd_processos pro\nLEFT JOIN app_fd_detalhe_processo det ON pro.c_detalhe_processo = det.id\nLEFT JOIN app_fd_anal_banc_processo abanc ON det.c_id_processo = abanc.c_id_processo\nWHERE abanc.c_data_2_emissao_fine > 0\nUNION\nSELECT pro.c_servico AS Servico,det.c_id_processo AS IDProc, det.c_nome_banco AS Banco, \ndet.c_forma_outorga as Forma_outorga, det.c_n_wf_pl as WF_PL,\nabanc.c_data_3_emissao_fine AS EmissaoFINE, abanc.c_origem_fine_3 AS OrigemFINE, 'Not Ok' AS Estado\nFROM app_fd_processos pro\nLEFT JOIN app_fd_detalhe_processo det ON pro.c_detalhe_processo = det.id\nLEFT JOIN app_fd_anal_banc_processo abanc ON det.c_id_processo = abanc.c_id_processo\nWHERE abanc.c_data_3_emissao_fine > 0\nUNION\nSELECT pro.c_servico AS Servico, det.c_id_processo AS IDProc, det.c_nome_banco AS Banco, \ndet.c_forma_outorga as Forma_outorga, det.c_n_wf_pl as WF_PL,\nabanc.c_data_4_emissao_fine AS EmissaoFINE, abanc.c_origem_fine_4 AS OrigemFINE, 'Not Ok' AS Estado\nFROM app_fd_processos pro\nLEFT JOIN app_fd_detalhe_processo det ON pro.c_detalhe_processo = det.id\nLEFT JOIN app_fd_anal_banc_processo abanc ON det.c_id_processo = abanc.c_id_processo\nWHERE abanc.c_data_4_emissao_fine > 0\nUNION\nSELECT pro.c_servico AS Servico, det.c_id_processo AS IDProc, det.c_nome_banco AS Banco, \ndet.c_forma_outorga as Forma_outorga, det.c_n_wf_pl as WF_PL,\nabanc.c_data_5_emissao_fine AS EmissaoFINE, abanc.c_origem_fine_5 AS OrigemFINE, 'Not Ok' AS Estado\nFROM app_fd_processos pro\nLEFT JOIN app_fd_detalhe_processo det ON pro.c_detalhe_processo = det.id\nLEFT JOIN app_fd_anal_banc_processo abanc ON det.c_id_processo = abanc.c_id_processo\nWHERE abanc.c_data_5_emissao_fine > 0\n\n\n"
        }
