    "rowActions": [{
        "className": "org.joget.plugin.enterprise.BeanShellDatalistAction",
        "icon": "<i class=\"fas fa-code\"><\/i>",
        "id": "rowAction_1",
        "label": "Bean Shell",
        "name": "Bean Shell Datalist Action",
        "properties": {
            "confirmation": "",
            "datalist_type": "row_action",
            "label": "Puxar Processo",
            "rules": [],
            "script": "import org.joget.workflow.model.service.WorkflowManager;\r\nimport org.joget.apps.app.service.AppUtil;\r\nimport org.joget.workflow.model.service.WorkflowUserManager;\r\nimport org.joget.apps.app.model.AppDefinition;\r\nimport javax.servlet.http.HttpServletRequest;\r\nimport org.joget.commons.util.LogUtil;\r\nimport org.joget.apps.datalist.model.DataListBinderDefaultimport;\r\nimport java.sql.Connection;\r\nimport java.sql.PreparedStatement;\r\nimport java.sql.ResultSet;\r\nimport java.sql.SQLException;\r\nimport javax.sql.DataSource;\r\nimport org.joget.commons.util.UuidGenerator;\r\n\r\npublic void puxarProcesso() {\r\n    String idProcesso=\"\";\r\n    String aid = \"\";\r\n    String resourceId = \"\";\r\n\r\n    for (String key : rowKeys) {\r\n        idProcesso = key;\r\n    }\r\n    \r\n    Connection con = null;\r\n    PreparedStatement stmt = null;\r\n    ResultSet rs = null;\r\n    \r\n    try{\r\n        DataSource ds = (DataSource)AppUtil.getApplicationContext().getBean(\"setupDataSource\");\r\n        con = ds.getConnection();\r\n        \r\n        if(!con.isClosed()){\r\n            String queryGetAid = \"SELECT a.id as 'Aid', sass.ResourceId as 'ResourceId' FROM shkprocesses p LEFT JOIN shkactivities a ON p.id = ProcessId LEFT JOIN shkprocessstates ps ON ps.oid = p.State LEFT JOIN SHKActivityStates acs ON acs.oid = a.State LEFT JOIN shkassignmentstable sass ON sass.ActivityId = a.id WHERE p.ActivityRequesterProcessId = ? AND a.id LIKE '%CHConclusoes%' AND a.id LIKE '%CHConclusoes%' AND ps.KeyValue LIKE 'open.running' AND (acs.KeyValue LIKE 'open.not_running.not_started' OR acs.KeyValue LIKE 'open.running');\";\r\n            stmt = con.prepareStatement(queryGetAid);\r\n            stmt.setObject(1, idProcesso);\r\n            rs = stmt.executeQuery();\r\n            \r\n            if(rs.next()) {\r\n                aid = rs.getObject(\"Aid\");\r\n                resourceId= rs.getObject(\"ResourceId\");\r\n                    \r\n                WorkflowManager wm = (WorkflowManager) AppUtil.getApplicationContext().getBean(\"workflowManager\");\r\n                WorkflowUserManager wum = (WorkflowUserManager) AppUtil.getApplicationContext().getBean(\"workflowUserManager\");\r\n                \r\n                //get the values from the \r\n                String replaceUser = resourceId;\r\n                String username = \"#currentUser.username#\";\r\n                \r\n                //getCurrentThreadUser() Method used by system to get current thread user\r\n                String currentUser = wum.getCurrentThreadUser();\r\n                wum.setCurrentThreadUser(replaceUser);\r\n                wum.setCurrentThreadUser(currentUser);\r\n                    \r\n                //Reassigns the assignment from a user (replaceUser) to another user (username)\r\n                wm.assignmentReassign(\"\", \"\", aid, username, replaceUser);\r\n                    \r\n                String queryGetNProcesso = \"SELECT c_n_processo FROM app_fd_detalhe_proce_procu WHERE c_id_processo = ?\";\r\n                stmt = con.prepareStatement(queryGetNProcesso);\r\n                stmt.setObject(1, idProcesso);\r\n                rs = stmt.executeQuery();\r\n                \r\n                String nProcesso = \"\";\r\n                if(rs.next()){\r\n                    nProcesso = rs.getObject(\"c_n_processo\");\r\n                }\r\n                \r\n                /*\r\n                    Atualizar histórico de processos puxados -> Procuradoria\r\n                \r\n                    String queryInsertHistorico = \"INSERT INTO app_fd_hist_puxar_procu(id, dateCreated, dateModified , createdBy, modifiedBy, c_id_processo, c_n_processo, c_gestor, c_data_puxar) VALUES (?,?,?,?,?,?,?,?,?);\";\r\n                    stmt = con.prepareStatement(queryInsertHistorico);\r\n                        \r\n                    String id = UuidGenerator.getInstance().getUuid();\r\n                    \r\n                    stmt.setObject(1, id);\r\n                    stmt.setObject(2, null);\r\n                    stmt.setObject(3,  null);\r\n                    stmt.setObject(4, \"#currentUser.username#\");\r\n                    stmt.setObject(5, \"#currentUser.username#\");\r\n                    stmt.setObject(6, idProcesso);\r\n                    stmt.setObject(7, nProcesso);\r\n                    stmt.setObject(8, username);\r\n                    stmt.setObject(9, \"#date.yyyy-mm-dd HH:mm#\");\r\n                    \r\n                    rs = stmt.executeQuery();\r\n                */\r\n            }\r\n        }\r\n            \r\n    }catch(Exception e){\r\n        LogUtil.error(\"Conclusões - Puxar Processo\", e, \"Error getting info from DB\");\r\n    }finally{\r\n        try{\r\n            if(rs != null) {\r\n                rs.close();\r\n            }\r\n                \r\n            if(stmt != null) {\r\n                stmt.close();\r\n            }\r\n            \r\n            if(con != null && !con.isClosed()) {\r\n                con.close();\r\n            }\r\n        }catch(Exception e){\r\n            LogUtil.error(appDef.toString(), e, \"Error closing DB connection\");\r\n        }\r\n    }    \r\n}\r\n\r\n\r\npuxarProcesso();"
        },
