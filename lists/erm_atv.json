    "binder": {
        "className": "org.joget.plugin.enterprise.JdbcDataListBinder",
        "properties": {
            "jdbcDatasource": "default",
            "optimisePaging": "",
            "primaryKey": "id",
            "sql": "SELECT sact.id as 'idAtividade', DATE_FORMAT(FROM_UNIXTIME(floor(sact.activated/1000)),\"%Y-%m-%d %H:%i\") as InicioAtividade, DATE_FORMAT(FROM_UNIXTIME(floor(sact.LastStateTime/1000)),\"%Y-%m-%d %H:%i\") as FimAtividade,\np.id, IF(sact.ResourceId IS NULL, IF(sass.ResourceId IS NULL,NULL, sass.ResourceId), sact.ResourceId) as 'Utilizador',\ndp.c_nome_banco AS Banco, dp.c_n_wf_pl AS WF_PL, dp.c_data_primeira_entrada as Data_1entrada, DATEDIFF(FROM_UNIXTIME(floor(sact.LastStateTime/1000)),FROM_UNIXTIME(floor(sact.activated/1000))) AS SLA\nFROM shkprocesses sp\nLEFT JOIN SHKActivities sact ON sact.ProcessId = sp.Id\nLEFT JOIN shkassignmentstable sass ON sass.ActivityId = sact.id\nLEFT JOIN app_fd_processos p on sp.ActivityRequesterProcessId = p.id\nLEFT JOIN app_fd_detalhe_processo dp ON p.c_detalhe_processo= dp.id\nWHERE sact.id LIKE '%filtro%' AND (sact.ResourceId IS NOT NULL OR sass.ResourceId IS NOT NULL)\nORDER BY Data_1entrada DESC\n\n\n\n"
        }
    },
