            "primaryKey": "id",
            "sql": "SELECT sact.id as 'idAtividade', p.id, IF(sact.ResourceId IS NULL, IF(sass.ResourceId IS NULL,NULL, sass.ResourceId), sact.ResourceId) as 'Utilizador', dp.c_nome_banco AS Banco, dp.c_n_wf_pl AS WF_PL, dp.c_data_primeira_entrada as Data_1entrada\nFROM shkprocesses sp\nLEFT JOIN SHKActivities sact ON sact.ProcessId = sp.Id\nLEFT JOIN shkassignmentstable sass ON sass.ActivityId = sact.id\nLEFT JOIN app_fd_processos p on sp.ActivityRequesterProcessId = p.id\nLEFT JOIN app_fd_detalhe_processo dp ON p.c_detalhe_processo= dp.id\nWHERE sact.id LIKE '%filtro%' AND (sact.ResourceId IS NOT NULL OR sass.ResourceId IS NOT NULL)\nORDER BY Data_1entrada DESC"
