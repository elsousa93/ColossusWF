            "primaryKey": "idProc",
            "sql": "SELECT p.dateCreated, ato.c_status, IF(ato.c_status = 'AGD', ato.c_dataOutorga, '-') as dataAgendamento, \np.id as idProc, dp.c_n_processo as NProc, dp.c_banco_fundo as Banco, dp.c_tipo_processo as TipoProcesso, sp.Id AS processId, sp.Name AS processName\nFROM shkprocesses sp\nLEFT JOIN shkprocesses sp2 on sp.ActivityRequesterProcessId = sp2.Id \nLEFT JOIN app_fd_processos_procu p on sp.ActivityRequesterProcessId = p.id\nLEFT JOIN app_fd_detalhe_proce_procu dp ON dp.id = p.c_detalhe_processo\nLEFT JOIN app_fd_atos ato ON ato.c_id_processo = p.id \nLEFT JOIN shkprocessstates sps ON sps.oid = sp.State\nLEFT JOIN app_fd_agendamento ag ON  ag.id = p.c_agendamento\nWHERE sps.KeyValue LIKE 'closed.completed'\nAND sp.id LIKE '%PCDProcuradoria%'\nAND (ato.dateCreated = (SELECT MAX(dateCreated) FROM app_fd_atos a2 WHERE a2.c_id_processo = p.id) || ato.dateCreated IS null)\n"
        }
    },
