    "binder": {
        "className": "org.joget.plugin.enterprise.JdbcDataListBinder",
        "properties": {
            "jdbcDatasource": "default",
            "optimisePaging": "",
            "primaryKey": "id",
            "sql": "(SELECT p.id, sact.Name AS Atividade, sp.Name AS Processo, dp.c_urgencia AS Urgencia, dp.c_nome_banco AS NomeBanco, dp.c_n_wf_banco AS NumeroWFBanco, p.DateCreated\nFROM shkprocesses sp\nLEFT JOIN app_fd_processos p ON p.id = sp.Id\nJOIN SHKActivities sact ON sact.ProcessId = sp.id\nJOIN SHKActivityStates ssta ON ssta.oid = sact.State\nJOIN SHKAssignmentsTable sass ON sact.Id = sass.ActivityId\nLEFT JOIN app_fd_detalhe_processo dp ON dp.id = p.c_detalhe_processo\nJOIN shkprocessstates sps ON sps.oid = sp.State)\nUNION \n(SELECT e.id, sact.Name AS Atividade, sp.Name AS Processo, dp.c_urgencia AS Urgencia, dp.c_nome_banco AS NomeBanco, dp.c_n_wf_banco AS NumeroWFBanco, e.DateCreated\nFROM app_fd_esclarecimentos e\nJOIN shkprocesses sp ON sp.Id = e.Id\nJOIN app_fd_processos p ON p.id = e.c_id_processo\nLEFT JOIN app_fd_detalhe_processo dp ON dp.id = p.c_detalhe_processo\nJOIN SHKActivities sact ON sact.ProcessId = e.id\nJOIN SHKActivityStates ssta ON ssta.oid = sact.State\nJOIN SHKAssignmentsTable sass ON sact.Id = sass.ActivityId\nJOIN shkprocessstates sps ON sps.oid = sp.State)"
        }
    },
