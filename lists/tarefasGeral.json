    "binder": {
        "className": "org.joget.plugin.enterprise.JdbcDataListBinder",
        "properties": {
            "jdbcDatasource": "default",
            "optimisePaging": "",
            "primaryKey": "IdProcesso",
            "sql": "(SELECT p.id as IdProcesso, sact.Name AS Atividade, sp.Name AS Processo, dp.c_urgencia AS Urgencia, dp.c_nome_banco AS NomeBanco, dp.c_n_wf_banco AS NumeroWFBanco, p.DateCreated\nFROM app_fd_processos p\nJOIN SHKActivities sact ON sact.ProcessId = p.id\nJOIN SHKActivityStates ssta ON ssta.oid = sact.State\nJOIN SHKAssignmentsTable sass ON sact.Id = sass.ActivityId\nJOIN shkprocesses sp ON p.id = sp.Id\nJOIN app_fd_detalhe_processo dp ON dp.id = p.c_detalhe_processo\nJOIN shkprocessstates sps ON sps.oid = sp.State\nWHERE ssta.KeyValue = 'open.not_running.not_started'\nAND sps.KeyValue = 'open.running')\nUNION \n(SELECT e.id as IdProcesso, sact.Name AS Atividade, sp.Name AS Processo, dp.c_urgencia AS Urgencia, dp.c_nome_banco AS NomeBanco, dp.c_n_wf_banco AS NumeroWFBanco, p.DateCreated\nFROM app_fd_esclarecimentos e\nJOIN app_fd_processos p ON p.id = e.c_id_processo\nJOIN app_fd_detalhe_processo dp ON dp.id = p.c_detalhe_processo\nJOIN SHKActivities sact ON sact.ProcessId = e.id\nJOIN SHKActivityStates ssta ON ssta.oid = sact.State\nJOIN SHKAssignmentsTable sass ON sact.Id = sass.ActivityId\nJOIN shkprocesses sp ON sp.Id = e.Id\nJOIN shkprocessstates sps ON sps.oid = sp.State\nWHERE ssta.KeyValue = 'open.not_running.not_started'\nAND sps.KeyValue = 'open.running')"
        }
    },
