    "binder": {
        "className": "org.joget.plugin.enterprise.JdbcDataListBinder",
        "properties": {
            "jdbcDatasource": "default",
            "optimisePaging": "",
            "primaryKey": "idAto",
            "sql": "SELECT p.id as idProc, ato.*, ato.id as idAto \nFROM app_fd_atos_procu ato \nLEFT JOIN app_fd_processos_procu p ON ato.c_id_processo = p.id\nLEFT JOIN app_fd_agendamento_procu a ON a.id = p.c_agendamento\nLEFT JOIN shkprocesses shk ON shk.ActivityRequesterProcessId  = p.id\nLEFT JOIN SHKActivities sact ON sact.ProcessId = shk.Id \nLEFT JOIN SHKActivityStates ssta ON ssta.oid = sact.State\nWHERE (ssta.oid = '1000001' OR ssta.oid = '1000003' OR ssta.oid = '1000005')\nAND ato.c_status = 'AGN'\nGROUP BY ato.id"
        }
    },
