            "primaryKey": "activity",
            "sql": "SELECT asst.activityid as 'activityId', a.`name` as 'activityName', proce.activityrequesterprocessId as 'processId',proce.name as 'processName',dp.c_urgencia as 'urgencia', dp.c_estado_contratacao as 'estado_contratacao', dp.c_origem_processo as 'origemProcesso', FROM_UNIXTIME(floor(a.activated/1000)) as 'dateCreated', asst.resourceid as 'resourceId', p.dateModified as \"dateModified\" \nFROM shkassignmentstable asst\nJOIN shkprocesses proce ON proce.id=asst.activityprocessid\nLEFT JOIN app_fd_processos p ON p.id = proce.activityrequesterprocessId\nLEFT JOIN app_fd_detalhe_processo dp ON p.c_detalhe_processo = dp.id\nJOIN shkactivities a ON a.id=asst.activityid\nWHERE asst.resourceid LIKE '#currentUser.id#'\nORDER BY p.dateModified DESC"
        }
    },
