            "primaryKey": "id",
            "sql": "SELECT qF.Ano, qF.Mes, qF.Banco, SUM(qf.FiNEs) AS 'TotalFines'\nFROM\n(SELECT det.c_n_wf_pl AS WF_PL,  YEAR(det.c_data_entrada) as Ano, Month(det.c_data_entrada) as Mes, det.c_nome_banco AS Banco,\nIF(DAY(abanc.c_data_5_emissao_fine)>0,5,IF(DAY(abanc.c_data_4_emissao_fine)>0,4,IF(DAY(abanc.c_data_3_emissao_fine)>0,3,IF(DAY(abanc.c_data_2_emissao_fine)>0,2,IF(DAY(abanc.c_data_emissao_fine)>0,1,0))))) AS FINEs\nFROM app_fd_processos pro\nLEFT JOIN app_fd_detalhe_processo det ON pro.c_detalhe_processo = det.id\nLEFT JOIN app_fd_anal_banc_processo abanc ON det.c_id_processo = abanc.c_id_processo\nWHERE det.c_data_entrada != '') AS qF\nGROUP BY qF.Ano, qF.Mes, qF.Banco\n\n\n\n\n"
        }
    },
