    "binder": {
        "className": "org.joget.plugin.enterprise.JdbcDataListBinder",
        "properties": {
            "jdbcDatasource": "default",
            "optimisePaging": "",
            "primaryKey": "IDProcesso",
            "sql": "SELECT DISTINCT p.id as IDProcesso, sp.Id, FROM_UNIXTIME(floor(sact.activated/1000)) as dateCreated, sact.Name AS Atividade, sp.Name AS Processo, dp.c_urgencia AS Urgencia, dp.c_nome_banco AS NomeBanco, dp.c_n_wf_banco AS NumeroWFBanco, dp.c_n_wf_pl as NumeroWFPL, dp.c_tipo_processo as TipoProcesso, dp.c_nome_cliente as NomeCliente, c_estado_contratacao AS EstadoContratacao, sact.Id AS ActivityId, from_unixtime(sact.LimitTime/1000, '%Y-%m-%d %H:%i') as limittime, sla.c_data_inicio as DataInicio,sla.c_data_fim as DataFim,dp.c_escalar_processo\nFROM shkprocesses sp\nLEFT JOIN app_fd_processos p on sp.ActivityRequesterProcessId = p.id\nLEFT JOIN shkprocessstates sps ON sps.oid = sp.State\nLEFT JOIN SHKActivities sact ON sact.ProcessId = sp.Id\nLEFT JOIN SHKActivityStates ssta ON ssta.oid = sact.State\nLEFT JOIN SHKAssignmentsTable sass ON sact.Id = sass.ActivityId\nLEFT JOIN app_fd_detalhe_processo dp ON dp.id = p.c_detalhe_processo\nLEFT JOIN app_fd_sla_atividade sla ON sla.c_id_processo = sp.Id\nWHERE sps.KeyValue LIKE 'open.running'\nAND (ssta.KeyValue LIKE 'open.not_running.not_started' OR ssta.KeyValue LIKE 'open.running')\nAND dp.c_escalar_processo LIKE IF('#currentUser.groups.name#' LIKE '%Coordenador%', 'coordenador',IF('#currentUser.groups.name#' LIKE '%DiretorCoordenador%','diretoresCoordenadores',IF('#currentUser.department.name#' LIKE '%PL_INT_ADM%','administracao','')))\nORDER BY Urgencia desc, LimitTime desc"
        }
    },
