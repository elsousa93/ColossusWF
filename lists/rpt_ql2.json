            "primaryKey": "id",
            "sql": "SELECT det.c_gestor_validacao_deposito,\nYEAR(ato.c_dataOutorga) AS AnoAgendamento, MONTH(ato.c_dataOutorga) AS  MesAgendamento, ato.c_status as StatusAgendamento, COUNT(det.id) AS Total\nFROM app_fd_processos pro\nLEFT JOIN app_fd_detalhe_processo det ON pro.c_detalhe_processo = det.id\nLEFT JOIN app_fd_agendamento ag ON pro.c_agendamento = ag.id\nLEFT JOIN app_fd_atos ato ON ato.c_id_agendamento = ag.id AND ato.c_status = 'OUT'\nWHERE ato.c_status != ''\nGROUP BY det.c_gestor_validacao_deposito, AnoAgendamento, MesAgendamento, StatusAgendamento\n\n\n\n\n"
        }
    },
