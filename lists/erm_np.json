            "optimisePaging": "",
            "primaryKey": "id",
            "sql": "SELECT lg.c_process_id AS IDProc,lg.c_campo AS Campo, lg.c_data_alteracao as Data_alteracao, lg.c_novo_valor as Novo_valor, det.c_bem_primeira AS Bem_primeira,\ndet.c_fase_processo AS Fase, det.c_n_wf_pl AS WF_PL, det.c_nome_banco AS Banco, det.c_tipo_processo AS Tipo_processo, det.c_forma_outorga as Forma_outorga, det.c_origem_processo AS Origem,\nSUM(IF(imo.c_imovel_banco='Sim',1,0)) AS Imovel_banco, pro.c_servico AS Servico\nFROM app_fd_audit_log lg\nLEFT JOIN app_fd_detalhe_processo det ON det.c_id_processo= lg.c_process_id\nLEFT JOIN app_fd_processos pro ON pro.c_detalhe_processo = det.id\nLEFT JOIN app_fd_imoveis imo ON det.c_id_processo = imo.c_id_processo\nWHERE lg.c_campo ='estado_contratacao' OR lg.c_campo='urgencia' OR lg.c_campo='urgencia_analise' OR lg.c_campo='urgencia_agendamento'\nGROUP BY IDProc, Campo, Data_alteracao, Novo_valor, Fase, WF_PL, Banco, Tipo_processo, Forma_outorga, Origem, Servico, Bem_primeira\n\n\n\n\n"
        }
    },
