            "primaryKey": "id",
            "sql": "SELECT COUNT(det.c_id_processo) as Processos, 'estado_contratacao' AS Campo, YEAR(det.dateCreated) AS Ano, MONTH(det.dateCreated) AS Mes, 'NOV' AS Novo_valor, \ndet.c_nome_banco AS Banco\nFROM app_fd_detalhe_processo det\nLEFT JOIN app_fd_imoveis imo ON det.c_id_processo = imo.c_id_processo\nGROUP BY Campo, Ano, Mes, Novo_valor, Banco\nUNION\nSELECT COUNT(lg.c_process_id) AS Processos,lg.c_campo AS Campo, YEAR(lg.c_data_alteracao) AS Ano, MONTH(lg.c_data_alteracao) AS Mes, lg.c_novo_valor as Novo_valor,\ndet.c_nome_banco AS Banco\nFROM app_fd_audit_log lg\nLEFT JOIN app_fd_detalhe_processo det ON det.c_id_processo= lg.c_process_id\nLEFT JOIN app_fd_imoveis imo ON det.c_id_processo = imo.c_id_processo\nGROUP BY Campo, Ano, Mes, Novo_valor, Banco\n\n\n\n\n"
        }
