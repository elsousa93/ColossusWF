                    {
                        "className": "org.joget.apps.form.lib.CustomHTML",
                        "properties": {
                            "autoPopulate": "",
                            "id": "apresentacao_campos",
                            "label": "",
                            "value": "<script>\n\n$(document).ready(function(){\n    var origem_processo = \"#form.detalhe_processo.origem_processo[{form.processos.detalhe_processo}]#\";\n    \n    $(\"th[id*=intervenientes_cliente_principal]\").hide();\n    $(\"span[id*=intervenientes_cliente_principal]\").parent().hide();\n    \n    //$(\"div#multiPaged_3_intervenientes_section5\").parent().hide();\n        \n    if(!(origem_processo === \"ACE\" || origem_processo === \"PRE\" || origem_processo === \"TRD\")){\n        \n        $(\"div[id*=compreendem_portugues]\").parent().remove();\n        $(\"div[id*=intervenientes_acidentais]\").parent().remove();\n        $(\"textarea[id*=observacoes_intervenientes]\").parent().remove();\n        \n        $(\"th[id*=codigo_interveniente]\").remove();\n        $(\"span[id*=codigo_interveniente]\").parent().remove();\n        \n        $(\"th[id*=numero_cc]\").remove();\n        $(\"span[id*=numero_cc]\").parent().remove();\n        \n        $(\"th[id*=intervenientes_validade]\").remove();\n        $(\"span[id*=intervenientes_validade]\").parent().remove();\n        \n        $(\"th[id*=intervenientes_nif]\").remove();\n        $(\"span[id*=intervenientes_nif]\").parent().remove();\n        \n        $(\"th[id*=intervenientes_naturalidade]\").remove();\n        $(\"span[id*=intervenientes_naturalidade]\").parent().remove();\n        \n        $(\"th[id*=intervenientes_morada]\").remove();\n        $(\"span[id*=intervenientes_morada]\").parent().remove();\n        \n    }\n    \n    \n    \n});\n    \n    \n    \n    \n    \n    \n<\/script>"
                        }
                    },
                    {
                        "className": "org.joget.apps.form.lib.CustomHTML",
                        "properties": {
                            "autoPopulate": "",
                            "id": "testePreencherCampo",
                            "label": "",
                            "value": "<script>\r\n   $(document).ready(function(){\r\n\r\n        //Descobrir qual a última linha editada \r\n        var data_linhaEditada = FormUtil.getGridCellValues(\"lista_intervenientes.variavel\");\r\n        var maxValue =  0;\r\n        var maxIndex = 0;\r\n        \r\n        data_linhaEditada.forEach(findIndex);\r\n        \r\n        function findIndex(item, index, arr){\r\n            if(item > maxValue){\r\n                maxIndex = index;\r\n            }\r\n        }\r\n        \r\n        //Consultar o campo código do interveniente e guardar valor\r\n        var data_Codigo = FormUtil.getGridCellValues(\"lista_intervenientes.codigo_interveniente\");\r\n        var nome_Preencher_No_Conjugue = data_Codigo[maxIndex];\r\n        \r\n        \r\n        //Consultar o campo conjugue preenchido e guardar valor\r\n        var data_Conjuge = FormUtil.getGridCellValues(\"lista_intervenientes.conjugue\");\r\n        var codigoIntervienteProcurar = data_Conjuge[maxIndex];\r\n        \r\n        \r\n        //Procurar valor encontrado na coluna do código do interveniente\r\n        var data_Codigo = FormUtil.getGridCellValues(\"lista_intervenientes.codigo_interveniente\");\r\n        var linha_a_substituir;\r\n        \r\n        data_Codigo.forEach(findLinhaASubstituir);\r\n        \r\n        function findLinhaASubstituir(item, index, arr){\r\n          if(item === codigoIntervienteProcurar){\r\n            linha_a_substituir = index;\r\n          }\r\n        }\r\n        \r\n        //Fazer o update do valor na célula correta\r\n        updateCell(\"lista_intervenientes\", \"conjugue\", nome_Preencher_No_Conjugue, linha_a_substituir); //row 2\r\n\r\n        //COMENTAR\r\n        function updateCell(fieldid, cellid, value, rowNo) {\r\n            var element = FormUtil.getField(fieldid);\r\n            gridSize = element.size();\r\n            var row = $(element).find(\"tr.grid-row:eq(\"+(rowNo-1)+\")\"); //0-based\r\n            var data = $(row).find(\"textfield\").val();\r\n            console.log(data.toString());\r\n            data = eval(\"[\"+data+\"]\")[0];\r\n            console.log(data.toString());\r\n\r\n            //data[\"conjugue\"] = value;\r\n            //$(element).enterpriseformgrid(\"fillValue\", element, row, JSON.stringify(data));\r\n        }\r\n        \r\n    });\r\n    \r\n    \r\n    //Incrementar Variavel\r\n     $(document).change(function(){\r\n        //Valor default sessionStorage\r\n        \r\n        if( sessionStorage.getItem(\"valor_variavel\") == null || sessionStorage.getItem(\"valor_variavel\") == \"NaN\"){\r\n            sessionStorage.setItem(\"valor_variavel\", \"0\");\r\n        }\r\n         \r\n        //Ir buscar informaçao de dentro e preencher o campo\r\n        var campoVariavel = FormUtil.getField(\"variavel_analisar\");\r\n        var valor = sessionStorage.getItem(\"valor_variavel\");\r\n        $(campoVariavel).val(valor); \r\n        \r\n        //Guardar valor do campo para passar para o form de dentro\r\n        var valorVariavel = FormUtil.getValue(\"variavel_analisar\");\r\n        sessionStorage.setItem(\"valor_variavel\", valorVariavel);\r\n\r\n     });\r\n         \r\n         \r\n<\/script>"
                        }
                    }
