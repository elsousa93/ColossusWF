                            "bb3e96c1-d61f-4c1c-f075-93ca3b6103dc": {}
                        },
                        "value": "<script type=\"text/javascript\">\r\n    $(document).change(function(){\r\n        var ficheiroId = \"nome_ficheiro\";\r\n\r\n        //get values\r\n        var doc = $(\"div.form-fileupload ul.form-fileupload-value li span\").text();\r\n          \r\n        //store the value to a field\r\n        $('[name='+ficheiroId+']:enabled').val(doc);\r\n    });\r\n<\/script>"
                    }
