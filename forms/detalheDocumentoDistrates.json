                {
                    "className": "org.joget.apps.form.lib.CustomHTML",
                    "properties": {
                        "autoPopulate": "",
                        "id": "abrirPdf",
                        "label": "",
                        "permission_rules": {
                            "905528b0-444a-645b-77f3-a7aa12612611": {},
                            "9de7f6a8-1d7c-13d5-e7fc-b76eb2395504": {},
                            "ac361275-02b8-99df-5b74-6bf70778aa79": {}
                        },
                        "value": "<div id=\"pdfPreview\" style=\"display:none;\">\r\n    <object\r\n      data=\"pdfFilePath\" width=\"100%\" height=\"100%\">\r\n      <iframe src=\"pdfFilePath\" width=\"100%\" height=\"100%\" style=\"border: none;\">\r\n        <p>Your browser does not support PDFs. <a href=\"pdfFilePath\">Download the PDF<\/a>.<\/p>\r\n      <\/iframe>\r\n    <\/object>\r\n<\/div>\r\n\r\n<script type=\"text/javascript\">\r\n$(function(){\r\n    $(\"ul.form-fileupload-value  li  a[class!='remove']\").click(function(e){\r\n        url = $(this).attr(\"href\");\r\n        if($(\"url:contains(.pdf.)\") || $(\"url:contains(.docx.)\")){\r\n            e.preventDefault();\r\n            showPopupActionDialog(url);\r\n        }\r\n    });\r\n});\r\n \r\nfunction showPopupActionDialog(url){\r\n    var wWidth = $(window).width();\r\n    var dWidth = wWidth * 0.9;\r\n    var wHeight = $(window).height();\r\n    var dHeight = wHeight * 0.9;\r\n     \r\n    var previewObj = $( \"#pdfPreview\" ).clone().html(function(index,html){\r\n        //console.log(\"ENTREI AQUI \" + url);\r\n        return html.replace(/pdfFilePath/g, url);\r\n    });\r\n     \r\n    var inputDialog = $(previewObj).dialog({\r\n      autoOpen: true,\r\n      height: dHeight,\r\n      width: dWidth,\r\n      modal: true,\r\n      buttons: [\r\n        {\r\n          text: \"Fechar\",\r\n          click: function() {\r\n            inputDialog.dialog( \"close\" );\r\n          }\r\n        }\r\n      ]\r\n    });\r\n}\r\n<\/script>"
