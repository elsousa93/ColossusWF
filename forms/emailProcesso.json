                        "id": "btn_enviar",
                        "label": "",
                        "value": "\n<!-- type=submit & name=assignmentComplete para concluir atividade do processo Esclarecimentos-->\n<input type=\"submit\" name=\"submit\" value=\"Enviar Email\" id=\"btn\" style=\"background-color:rgba(0,0,0,0);\"/>\n<button type=\"button\" onclick=\"myFunction()\">ABRIR EMAIL<\/button>\n<script type=\"text/javascript\">\n    var url, value, action, formAction;\n    $(document).ready(function(){\n        $(\"#submit\").remove();\n        value = \"#requestParam.id_atividade#\"; //get activity id do processo \"pai\"\n        action= \"#requestParam._action#\";\n        url = \"http://192.168.0.104:8080/jw/web/userview/workflowPL/UV_PL_Geral/_/filaTrabalho?_mode=assignment&activityId=\" + value; //criar url inserindo activity id corrente do processo\";\n        \n        \n        \n        if(action===\"submit\"){\n            url = localStorage.getItem(\"redirectUrl\");\n            console.log(url);\n           redirect();\n        }\n        if(value===\"\"){\n           $(\"#btn\").remove();\n        }else{\n            localStorage.setItem(\"redirectUrl\",url); \n        }\n    });\n    \n    \nfunction myFunction() {\n\n    var mailHtm =$(\"div[name=mensagem] textarea[id*=mensagem]\").val();\n    var emailto = $(\"input#destinatarios\").val();\n    var emailsubject = \"Email-\" + $(\"input#assunto\").val();\n    var emlCont = 'To: '+emailto + '\\n';\n\n    emlCont += 'Subject: '+emailsubject+'\\n';\n    emlCont += 'X-Unsent: 1'+'\\n';\n\n    emlCont += 'Content-Type: text/html'+'\\n';\n    emlCont += ''+'\\n';\n\n    emlCont += \"<!DOCTYPE html><html><head><\/head><body>\" + mailHtm + \"<\/body><\/html>\";\n   // console.log(emlCont) ;\n    var textFile = null;\n    var data = new Blob([emlCont], {type: 'text/plainText'});\n    if (textFile !== null) {\n            window.URL.revokeObjectURL(textFile);\n    }\n\n    textFile = window.URL.createObjectURL(data);\n    //console.log(textFile);\n\n    var a = document.createElement('a'); //make a link in document\n    var linkText = document.createTextNode('fileLink');\n    a.appendChild(linkText) ;\n\n    a.href = textFile;\n\n    a.id = 'fileLink';\n    a.download = emailsubject+\".eml\"; //'filenameTest.eml' ;\n    a.style.visibility = \"hidden\";\n\n    document.body.appendChild(a) ;\n\n    document.getElementById('fileLink').click();\n}\n    \n    function redirect(){\n        return window.location.href=url;\n    }\n\n<\/script>"
