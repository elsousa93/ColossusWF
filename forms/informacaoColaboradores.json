                            "optionsBinder": {
                                "className": "org.joget.apps.form.lib.BeanShellFormBinder",
                                "properties": {
                                    "cacheInterval": "",
                                    "script": "import org.joget.apps.app.service.AppUtil;\r\nimport org.joget.apps.form.model.Element;\r\nimport org.joget.apps.form.model.FormData;\r\nimport org.joget.apps.form.model.FormRow;\r\nimport org.joget.apps.form.model.FormRowSet;\r\nimport org.joget.apps.form.service.FormUtil;\r\n\r\npublic FormRowSet loadAux(FormRowSet rows){\r\n    \r\n    String cUserDepartmentDesc = \"#user.USERNAME.department.description#\";\r\n    String cUserDepartmentID = \"#user.USERNAME.department.id#\";\r\n    \r\n    String[] cUserDepDesc = cUserDepartmentDesc.split(\";\");\r\n    String[] cUserDepID = cUserDepartmentID.split(\";\");\r\n\r\n    //Get the departmants the current user is tied to\r\n    for(int i = 0; i < cUserDepDesc.length; i++ ){\r\n        FormRow option = new FormRow();\r\n        option.setProperty(FormUtil.PROPERTY_VALUE, cUserDepID[i]);\r\n        option.setProperty(FormUtil.PROPERTY_LABEL, cUserDepDesc[i]);\r\n        rows.add(option);\r\n    }\r\n\treturn rows;\r\n}\r\n \r\npublic FormRowSet load(String[] values) {\r\n    FormRowSet rows = new FormRowSet();\r\n    //System.out.println(values[0]);\r\n    String user = null;\r\n    System.out.println(\"user\");\r\n\r\n    if (values != null && values.length > 0) {\r\n        user = values[0];\r\n        System.out.println(user);\r\n        FormRow option = new FormRow();\r\n        option.setProperty(FormUtil.PROPERTY_VALUE, user);\r\n        option.setProperty(FormUtil.PROPERTY_LABEL, user);\r\n        rows.add(option);\r\n    }else{\r\n        FormRow option = new FormRow();\r\n        option.setProperty(FormUtil.PROPERTY_VALUE, \"\");\r\n        option.setProperty(FormUtil.PROPERTY_LABEL, \"\");\r\n        rows.add(option);\r\n    }\r\n    \r\n    /**\r\n    String value = \"#form.esclarecimentos.operativa_remetente#\";\r\n    String hash = \"$form.esclarecimentos.operativa_remetente$\";\r\n    \r\n    //value = hash.replaceAll(\"\\\\$\", \"#\");\r\n    System.out.println(value);\r\n    value = AppUtil.processHashVariable(value,null,null,null);\r\n    \r\n    System.out.println(value);\r\n    \r\n    String subValue = value.substring(1,value.length()-1);\r\n    String compare = hash.substring(1,hash.length()-1);\r\n    System.out.println(compare);\r\n    System.out.println(subValue);\r\n\r\n    if(subValue.equals(compare)){\r\n        rows = loadAux(rows);\r\n        System.out.println(\"1\");\r\n    }else{\r\n\t    System.out.println(\"2\");\r\n\r\n\t\tFormRow option = new FormRow();\r\n\t\toption.setProperty(FormUtil.PROPERTY_VALUE, value);\r\n\t\toption.setProperty(FormUtil.PROPERTY_LABEL, value);\r\n\t\trows.add(option);**/\r\n    \r\n    return rows;\r\n    }\r\n    \r\n    //call load method with injected variable\r\n    return load(values);",
