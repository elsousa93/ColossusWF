                            "value": "<script>\n    $(document).ready(function(){\n        \n\n        $(\"select[id*=estado_agendamento]\").prop('disabled', true);\n        if($(\"input[id*=flag_agendado]\").val() === '1'){\n            $(\"div[id*=agendamentoProcuradoria_agendamento]\").hide();\n        }else{\n            $(\"div[id*=mensagemAgendamento]\").hide();\n        }\n        \n        \n        \n        $(\"input#agendar\").click(function(){\n            $.ajax({\n                type:\"POST\",\n                url: \"/jw/api/form/atosAgendamento\",\n                headers:{\n                    \"api_id\": \"#envVariable.apiIdComunicacaoMediaView#\",\n                    \"api_key\": \"#envVariable.apiKeyComunicacaoMediaView#\",\n                    \"accept\":\"application/json\",\n                    \"Content-Type\": \"application/json\"\n                },\n                data: JSON.stringify({\n                                \"id_processo\": \"#form.processos_procu.id#\",\n                                \"id_agendamento\": \"#form.processos_procu.agendamento#\",\n                                \"procurador\": \"Procurador Exemplo\",\n                                \"dataOutorga\": \"2023-02-07\",\n                                \"horaOutorga\": \"10:43\",\n                                \"localOutorga\": \"Local Exemplo\",\n                                \"status\": \"EMAGE\"\n                            }),\n                success: function(data){\n                    //Falta chamar API MediaView\n                    \n                    updateEstadoAgendamento();\n                }\n            });\n        });\n        \n        \n        \n    });\n    \n\n    \n    \nfunction updateEstadoAgendamento(){\n        $.ajax({\n                type:\"POST\",\n                url: \"/jw/api/form/agendamentoProcuradoria/saveOrUpdate\",\n                headers:{\n                    \"api_id\": \"#envVariable.apiIdComunicacaoMediaView#\",\n                    \"api_key\": \"#envVariable.apiKeyComunicacaoMediaView#\",\n                    \"accept\":\"application/json\",\n                    \"Content-Type\": \"application/json\"\n                },\n                data: JSON.stringify({\n                                \"id\": \"#form.processos_procu.agendamento#\",\n                                \"flag_agendado\": \"1\"\n                            }),\n                success: function(data){\n                    location.reload();\n                }\n            });\n    }\n \n    \n<\/script>\n<input id=\"agendar\" type=\"button\" value=\"PEDIR AGENDAMENTO\">"
