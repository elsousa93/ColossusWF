                            "id": "field7",
                            "label": "",
                            "value": "<script>\n    $(document).ready(function(){\n        \n\n        $(\"select[id*=estado_agendamento]\").prop('disabled', true);\n        if($(\"input[id*=flag_agendado]\").val() === '1'){\n            $(\"div[id*=agendamentoProcuradoria_agendamento]\").hide();\n        }else{\n            $(\"div[id*=mensagemAgendamento]\").hide();\n        }\n        \n        \n        $(\"input#agendar\").click(function(){\n            var urgencia = $(\"div[id*=urgencia] input:checked\").val();\n            var dataSolicitada=$(\"input[id*=data_solicitada]\").val();\n            var horaSolicitada=$(\"input[id*=hora_solicitada]\").val();\n            var observacoes=$(\"textarea[id*=observacoes_agendamento]\").val();\n            $.ajax({\n                type:\"POST\",\n                url: \"/jw/api/form/atosAgendamento\",\n                headers:{\n                    \"api_id\": \"#envVariable.apiIdComunicacaoMediaView#\",\n                    \"api_key\": \"#envVariable.apiKeyComunicacaoMediaView#\",\n                    \"Accept\":\"application/json\",\n                    \"Content-Type\": \"application/json\"\n                },\n                data: JSON.stringify({\n                                \"id_processo\": \"#form.processos_procu.id#\",\n                                \"id_agendamento\": \"#form.processos_procu.agendamento#\",\n                                \"urgencia\": 'SIM',\n                                \"data_solicitada\":dataSolicitada,\n                                \"hora_solicitada\":horaSolicitada,\n                                \"observacoes_agendamento\":observacoes,\n                                \"procurador\": \"ProcuradorExemplo\",\n                                \"dataOutorga\": \"2023-02-15\",\n                                \"horaOutorga\": \"15:00\",\n                                \"localOutorga\": \"LocalExemplo\",\n                                \"status\": \"AGD\"\n                            }),\n                success: function(response){\n                    //Falta chamar API MediaView\n                    var id = response.id;\n                    updateEstadoAgendamento();\n                }\n            });\n        });\n        \n        \n        \n    });\n    \n\n    \n    \nfunction updateEstadoAgendamento(){\n        $.ajax({\n                type:\"POST\",\n                url: \"/jw/api/form/agendamentoProcuradoria/saveOrUpdate\",\n                headers:{\n                    \"api_id\": \"#envVariable.apiIdComunicacaoMediaView#\",\n                    \"api_key\": \"#envVariable.apiKeyComunicacaoMediaView#\",\n                    \"accept\":\"application/json\",\n                    \"Content-Type\": \"application/json\"\n                },\n                data: JSON.stringify({\n                                \"id\": \"#form.processos_procu.agendamento#\",\n                                \"flag_agendado\": \"1\"\n                            }),\n                success: function(data){\n                    location.reload();\n                }\n            });\n    }\n \n    \n<\/script>\n<input id=\"agendar\" type=\"button\" value=\"PEDIR AGENDAMENTO\">"
