                        "value": "<script>\n    $(document).change(function(){\n        atualizarVisibilidadeBCP();\n        atualizarVisibilidadesMTG();\n        atualizarVisibilidadesABN();\n        atualizarVisibilidadesBIC();\n    });\n    \n    \n    \n    function atualizarVisibilidadeBCP(){\n        var nome_banco = '#form.detalhe_proce_dist.nome_banco[{form.processos_distrates.detalhe_processo}]#';\n        if(nome_banco==='BCP'){\n            $(\"input[id*=balcaoEnvio]\").parent().hide();\n        }\n    }\n    \n    function atualizarVisibilidadesMTG(){\n        var nome_banco = '#form.detalhe_proce_dist.nome_banco[{form.processos_distrates.detalhe_processo}]#';\n        if(nome_banco==='MTG'){\n            $(\"input[id*=balcaoEnvio]\").parent().hide();\n        }\n    }\n    \n    function atualizarVisibilidadesABN(){\n        var nome_banco = '#form.detalhe_proce_dist.nome_banco[{form.processos_distrates.detalhe_processo}]#';\n       \n        if(nome_banco==='ABN'){\n            $(\"input[id*=correios_origem]\").parent().hide();\n        }\n    }\n    \n    function atualizarVisibilidadesBIC(){\n        var nome_banco = '#form.detalhe_proce_dist.nome_banco[{form.processos_distrates.detalhe_processo}]#';\n    \n        if(nome_banco==='BIC'){\n            $(\"input[id*=balcaoEnvio]\").parent().hide();\n        }\n    }\n<\/script>\n\n\n "
