                    {
                        "className": "org.joget.apps.form.lib.CustomHTML",
                        "properties": {
                            "autoPopulate": "",
                            "id": "valor_cv_verdadeiro",
                            "label": "",
                            "permissionHidden": "",
                            "permission_rules": {
                                "0d6bd36c-8423-b830-526f-d71499d72a55": {},
                                "1405d883-3f63-bc1e-7949-1ce7e4457892": {},
                                "276487d8-0482-7519-09fd-807e28473a83": {},
                                "740d9ecd-911b-7df4-474a-0d7f0cf81147": {},
                                "79f8c35b-a7ad-ec7e-b9d8-f656d6176fa7": {},
                                "7cb0a003-f9de-cb30-4a9f-c17080392739": {
                                    "permissionHidden": "",
                                    "readonly": ""
                                },
                                "7f975a7f-0e81-1fe8-a41b-b5ae7202b9be": {},
                                "8ed12735-d92c-74bd-444f-888db20bd46a": {},
                                "aea23972-1f73-2c75-befa-3c2288f6d5bb": {}
                            },
                            "value": "<script type=\"text/javascript\">\r\n\r\n    $('#valor_cv').ready(function(){\r\n       \r\n        //const getValueInput = () =>{\r\n        //  let number = document.getElementById(\"valor_cv\").value; \r\n        //  document.getElementById(\"valor_cv\").innerHTML = number; \r\n        //}\r\n        \r\n        var f1 = FormUtil.getField(\"valor_cv\");\r\n        var number = FormUtil.getValue(\"valor_cv\");\r\n        \r\n        if(number != null && number !== \"\"){\r\n            number = number.replaceAll('NaN', \"0\");\r\n            number = number.replaceAll('.', \"\");\r\n            number = number.replaceAll('\u20ac', \"\");\r\n            number = number.replaceAll('Eur', \"\");\r\n            number = number.replaceAll('EUR', \"\");\r\n            number = number.replaceAll(',', '.');\r\n            \r\n            let num = new Intl.NumberFormat('pt-PT', { style: 'currency', currency: 'EUR' }).format(number);\r\n        \r\n            num = num.replace(/\\s/g, '.');\r\n            num = num.replaceAll('.\u20ac', ' EUR');\r\n            \r\n            $(f1).val(num);\r\n        }else{\r\n            //do nothing\r\n        }  \r\n        \r\n        $(\"#submit\").val(\"Submeter\");\r\n    });\r\n    \r\n    \r\n<\/script> "
                        }
                    },
                    {
                        "className": "org.joget.apps.form.lib.CustomHTML",
                        "properties": {
