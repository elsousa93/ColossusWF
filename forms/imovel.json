                            "id": "apresentacao_campos",
                            "label": "",
                            "permission_rules": {"7cb0a003-f9de-cb30-4a9f-c17080392739": {}},
                            "value": "<script>\n    \n    $(document).ready(function(){\n        var tipoProcesso = \"#form.detalhe_processo.tipo_processo[{form.processos.detalhe_processo}]#\";\n        \n        \n     \n        /**Falta corrigir valor do tipo de processo CVMHFiança Judicial no if**/\n    \n        if(!(tipoProcesso.includes(\"+CV+\") ||  tipoProcesso.includes(\"+CV12+\") || tipoProcesso.includes(\"+CVD12I+\") || tipoProcesso.includes(\"+CVD12IE+\") || \n        tipoProcesso.includes(\"+CVMH+\") || tipoProcesso.includes(\"+CVMHIE+\") || tipoProcesso.includes(\"+CVMH12+\") || tipoProcesso.includes(\"+CVMHJ+\") || tipoProcesso.includes(\"+CVMHF+\") \n        || tipoProcesso.includes(\"+CVMHFIE+\") || tipoProcesso.includes(\"+CVMHF12+\") || tipoProcesso.includes(\"+CVMHJ+\"))){\n            \n            $(\"div [id^=direito_preferencia]\").parent().remove();\n            $(\"div [id^=imovel_banco]\").parent().remove();\n            $(\"input#valor_cv\").parent().remove();\n        }\n        \n\n       \n    });\n    \n    \n    \n    \n    \n    \n<\/script>"
                        }
