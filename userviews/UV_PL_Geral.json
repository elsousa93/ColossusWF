                "horizontal_menu": "",
                "inbox": "all",
                "js": "var list, customInbox;\n$(document).ready(function(){\n    list=$(\".nav.pull-right\");\n    customInbox='<li class=\"custom-inbox-notification\" data-url=\"/jw/web/json/plugin/org.joget.plugin.enterprise.ProgressiveTheme/service?_a=getAssignment\" dropdown><a data-toggle=\"dropdown\" class=\"btn dropdown-toggle waves-effect btn waves-button waves-float\"><i class=\"fa fa-tasks white\"><\/i><span class=\"badge red\"><\/span><\/a><ul class=\"dropdown-menu notifications\"><li class=\"dropdown-menu-title\"><span>You have <strong class=\"count\"><\/strong> assignments.<\/span><a href=\"#\" class=\"refresh\"><i class=\"fa fa-refresh\"><\/i><\/a><\/li><li class=\"loading\" style=\"display: none;\"><li><a href=\"/jw/web/userview/workflowPL/UV_PL_Geral/_/_ja_inbox\" class=\"dropdown-menu-sub-footer\">View all assignments<\/a><\/li><\/ul><\/li>';\n    list.append(customInbox);\n    loadInbox();\n});\n\n!function($){\n    function loadInbox() {\n            if ($(\".custom-inbox-notification\").length === 1) {\n                loadCustomInboxData();\n                \n                $(\".custom-inbox-notification .refresh\").on(\"click\", function(e) {\n                    e.preventDefault();\n                    loadCustomInboxData();\n                    return false;\n                });\n            }\n    }\n        \n    \n        function loadCustomInboxData() {\n            console.log(\"TEste\");\n            $(\".custom-inbox-notification .loading\").show();\n            var url = $(\".inbox-notification\").data(\"url\");\n            $.getJSON(url + \"&_t=\" + (new Date()).getTime(), {}, function(data) {\n                var count = 0;\n                if (data.count !== undefined) {\n                    count = data.count;\n                }\n                $(\".custom-inbox-notification > a > .badge\").text(count);\n                $(\".custom-inbox-notification .dropdown-menu-title .count\").text(count);\n                $(\".custom-inbox-notification > ul > li.task\").remove();\n                if (data.data) {\n                    var footer = $(\".custom-inbox-notification > ul .dropdown-menu-sub-footer\").parent();\n                    var link = $(\".custom-inbox-notification > ul .dropdown-menu-sub-footer\").attr(\"href\");\n                    $.each(data.data, function(i, d) {\n                        var html = \"<li class=\\\"task\\\"><a href=\\\"\" + link + \"?_mode=assignment&activityId=\" + d.activityId + \"\\\">\";\n                        html += \"<span class=\\\"header\\\">\" + d.activityName + \"<\/span>\";\n                        html += \"<span class=\\\"message\\\">\" + d.processName + \"<\/span><span class=\\\"time\\\">\" + d.dateCreated + \"<\/span>\";\n                        html += \"<\/a><\/li>\";\n                        footer.before($(html));\n                    });\n                }\n                $(\".custom-inbox-notification .loading\").hide();\n            });\n        }\n}(window.jQuery);\n        \n        ",
